@model StepUpPL.Models.FacultiesDTO1

@{
    ViewBag.Title = "Faculty";
    Layout = "~/Views/Shared/_newui.cshtml";
}

<h2>Faculty Login</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
       
        <hr/>
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div style="text-align:left" class="form-group">
            @Html.LabelFor(model => model.Faculty_PS, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Faculty_PS, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Faculty_PS, "", new { @class = "text-danger" })
            </div>
        </div>

        <div style="text-align:left" class="form-group">
            @Html.LabelFor(model => model.Faculty_Password, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.Faculty_Password, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Faculty_Password, "", new { @class = "text-danger" })
            </div>
        </div>

        <div style="text-align:left" class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Login" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back", "LoginDet")
</div>
